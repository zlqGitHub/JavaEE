<struts>
	<package name="regist" extends="struts-default">
		
		<action name="registAction" class="nuc.sw.dangdang.action.RegistAction" method="registMethod">
			<result name="input">/regist.jsp</result>
			<result name="registOK">
				/success.jsp
			</result>
